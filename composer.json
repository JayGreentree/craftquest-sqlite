{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/ckeditor": "^4.0.0",
    "craftcms/cms": "^5.0.0",
    "craftcms/redactor": "4.0.0",
    "nystudio107/craft-twig-base-templates": "^1.0",
    "nystudio107/twig-bundle-installer": "^1.1",
    "vlucas/phpdotenv": "^5.4.0",
    "yiisoft/db-sqlite": "^1.2"
  },
  "require-dev": {
    "craftcms/generator": "2.0.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true,
      "nystudio107/twig-bundle-installer": true
    },
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    }
  }
}
